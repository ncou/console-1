    "homepage": "https://github.com/soliphp/console",
    "support": {
        "issues": "https://github.com/soliphp/console/issues",
        "source": "https://github.com/soliphp/console"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "ueaner",
            "email": "ueaner@gmail.com"
        }
    ],
    "require": {
